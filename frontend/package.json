{
  "name": "obs",
  "version": "0.0.1",
  "private": true,
  "sideEffects": true,
  "scripts": {
    "build": "rimraf ../public/{js,css,img} && vue-cli-service build --no-clean",
    "dev": "rimraf ../public/{js,css,img} && vue-cli-service build --mode development --no-clean --watch",
    "lint": "vue-cli-service lint",
    "vue-i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/lang/*.?(json|yml|yaml|js)'"
  },
  "dependencies": {
    "@carbon/charts-vue": "^0.54.10",
    "@limanmys/carbon-vue": "^2.42.5",
    "liquor-tree": "^0.2.70",
    "luxon": "^1.28.0",
    "prismjs": "^1.27.0",
    "vue": "2.6.14",
    "vue-i18n": "^8.22.1",
    "vue-page-transition": "^0.2.2",
    "vue-prism-editor": "^1.3.0",
    "vue-router": "3.5.3",
    "vue2-transitions": "0.3.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "chokidar": "^3.5.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^6.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue-i18n-extract": "^2.0.4",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-undef": "off",
      "no-console": "off",
      "vue/no-unused-components": "off",
      "vue/no-dupe-keys": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
